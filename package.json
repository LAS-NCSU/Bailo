  "name": "bailo",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "build": "next build && tsc --project tsconfig.server.json",
    "build_next": "next build",
    "build_server": "tsc --project tsconfig.server.json",
    "certs": "mkdir -p certs && openssl genrsa -out certs/key.pem 2048 && openssl req -new -x509 -key certs/key.pem -out certs/cert.pem -config certs/san.cnf -extensions 'v3_req' -days 360",
    "check-style": "prettier --check .",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "dev": "nodemon",
    "dev2": "TS_NODE_PROJECT=tsconfig.server.json ts-node-dev --ignore-watch '^(?!server)' --watch server --respawn --rs server/index.ts",
    "lint": "next lint",
    "script": "TS_NODE_FILES=true TS_NODE_TRANSPILE_ONLY=true TS_NODE_PROJECT=tsconfig.server.json node --trace-warnings -r ts-node/register -r tsconfig-paths/register server/scripts/runScript.ts",
    "start": "cross-env NODE_ENV=production node dist/server/index.js",
    "style": "prettier --write .",
    "test": "jest --bail"
  },
